{{#each blogs as |blog|}}
<div class="card">
    <div class="card-body">
        <div class="d-flex justify-content-between">
            <a href="/blogs/{{blog.id}}"><h2 class="card-title">{{blog.title}} by {{blog.User.name}}</h2></a>
            <p class="card-subtitle">{{blog.createdAt}}</p>
        </div>
        <div class="card-text">
            <p>{{blog.content}}</p>
        </div>
    </div>
</div>
{{/each}}